{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "",
    "projects": {
        "art-catalogue": {
            "root": "apps/art-catalogue/",
            "sourceRoot": "apps/art-catalogue/src",
            "projectType": "application",
            "prefix": "art-catalogue",
            "schematics": {
                "@nrwl/schematics:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/art-catalogue",
                        "index": "apps/art-catalogue/src/index.html",
                        "main": "apps/art-catalogue/src/main.ts",
                        "polyfills": "apps/art-catalogue/src/polyfills.ts",
                        "tsConfig": "apps/art-catalogue/tsconfig.app.json",
                        "assets": [
                            "apps/art-catalogue/src/favicon.ico",
                            "apps/art-catalogue/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "apps/art-catalogue/src/styles.scss"
                        ],
                        "scripts": [],
                        "es5BrowserSupport": true
                    },
                    "configurations": {
                        "develop": {},
                        "qa": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/art-catalogue/src/environments/environment.ts",
                                    "with": "apps/art-catalogue/src/environments/environment.qa.ts"
                                }
                            ]
                        },
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/art-catalogue/src/environments/environment.ts",
                                    "with": "apps/art-catalogue/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ]
                        },
                        "myoenconfig": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/art-catalogue/src/environments/environment.ts",
                                    "with": "apps/art-catalogue/src/environments/environment.onprem.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "art-catalogue:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "art-catalogue:build:production"
                        },
                        "qa": {
                            "browserTarget": "art-catalogue:build:qa"
                        },
                        "develop": {
                            "browserTarget": "art-catalogue:build:develop"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "art-catalogue:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/art-catalogue/tsconfig.app.json",
                            "apps/art-catalogue/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/art-catalogue/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "apps/art-catalogue/src/test.ts",
                        "tsConfig": "apps/art-catalogue/tsconfig.spec.json",
                        "karmaConfig": "apps/art-catalogue/karma.conf.js",
                        "polyfills": "apps/art-catalogue/src/polyfills.ts",
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
                        ],
                        "scripts": [],
                        "assets": []
                    }
                }
            }
        },
        "art-catalogue-e2e": {
            "root": "apps/art-catalogue-e2e",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "apps/art-catalogue-e2e/protractor.conf.js",
                        "devServerTarget": "art-catalogue:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "art-catalogue:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "apps/art-catalogue-e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/art-catalogue-e2e/**"
                        ]
                    }
                }
            }
        },
        "api-interfaces": {
            "projectType": "library",
            "root": "libs/api-interfaces",
            "sourceRoot": "libs/api-interfaces/src",
            "prefix": "web-devops-workshop",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/api-interfaces/tsconfig.lib.json",
                            "libs/api-interfaces/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!libs/api-interfaces/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/api-interfaces/src/test.ts",
                        "tsConfig": "libs/api-interfaces/tsconfig.spec.json",
                        "karmaConfig": "libs/api-interfaces/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@nrwl/angular:component": {
                    "styleext": "scss"
                }
            }
        },
        "decorators": {
            "projectType": "library",
            "root": "libs/decorators",
            "sourceRoot": "libs/decorators/src",
            "prefix": "web-devops-workshop",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/decorators/tsconfig.lib.json",
                            "libs/decorators/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!libs/decorators/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/decorators/src/test.ts",
                        "tsConfig": "libs/decorators/tsconfig.spec.json",
                        "karmaConfig": "libs/decorators/karma.conf.js"
                    }
                }
            },
            "schematics": {
                "@nrwl/angular:component": {
                    "styleext": "scss"
                }
            }
        },
        "art-api": {
            "root": "apps/art-api",
            "sourceRoot": "apps/art-api/src",
            "projectType": "application",
            "prefix": "art-api",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/art-api",
                        "main": "apps/art-api/src/main.ts",
                        "tsConfig": "apps/art-api/tsconfig.app.json",
                        "assets": [
                            "apps/art-api/src/assets"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/art-api/src/environments/environment.ts",
                                    "with": "apps/art-api/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "art-api:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/art-api/tsconfig.app.json",
                            "apps/art-api/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/art-api/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/art-api/jest.config.js",
                        "tsConfig": "apps/art-api/tsconfig.spec.json"
                    }
                }
            }
        },
        "angular-cypress-unit": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/angular-cypress-unit",
            "sourceRoot": "apps/angular-cypress-unit/src",
            "prefix": "web-devops-workshop",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/angular-cypress-unit",
                        "index": "apps/angular-cypress-unit/src/index.html",
                        "main": "apps/angular-cypress-unit/src/main.ts",
                        "polyfills": "apps/angular-cypress-unit/src/polyfills.ts",
                        "tsConfig": "apps/angular-cypress-unit/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/angular-cypress-unit/src/favicon.ico",
                            "apps/angular-cypress-unit/src/assets"
                        ],
                        "styles": [
                            "apps/angular-cypress-unit/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/angular-cypress-unit/src/environments/environment.ts",
                                    "with": "apps/angular-cypress-unit/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "angular-cypress-unit:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "angular-cypress-unit:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "angular-cypress-unit:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/angular-cypress-unit/tsconfig.app.json",
                            "apps/angular-cypress-unit/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/angular-cypress-unit/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/angular-cypress-unit/jest.config.js",
                        "tsConfig": "apps/angular-cypress-unit/tsconfig.spec.json",
                        "passWithNoTests": true,
                        "setupFile": "apps/angular-cypress-unit/src/test-setup.ts"
                    }
                }
            }
        },
        "angular-cypress-unit-e2e": {
            "root": "apps/angular-cypress-unit-e2e",
            "sourceRoot": "apps/angular-cypress-unit-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/angular-cypress-unit-e2e/cypress.json",
                        "tsConfig": "apps/angular-cypress-unit-e2e/tsconfig.e2e.json",
                        "devServerTarget": "angular-cypress-unit:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "angular-cypress-unit:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/angular-cypress-unit-e2e/tsconfig.e2e.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/angular-cypress-unit-e2e/**"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "warnings": {
            "typescriptMismatch": false,
            "versionMismatch": false
        },
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/schematics:application": {
            "style": "scss",
            "framework": "angular"
        },
        "@nrwl/schematics:library": {
            "style": "scss"
        },
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "defaultProject": "art-catalogue"
}
